{
    "compilerOptions": {
      "target": "es6",
      "lib": ["es5", "es6", "dom"],
      "types": ["cypress"],
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "strict": true
    },
    "include": [
      "cypress/**/*.ts",
      "cypress/**/*.js"
    ]
  }
  